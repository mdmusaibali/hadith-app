{
  "expo": {
    "name": "hadith-app",
    "slug": "hadith-app",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/Hadith.png",
    "scheme": "com.hadith.musaib",
    "splash": {
      "image": "./assets/splash1.png",
      "resizeMode": "contain",
      "backgroundColor": "#D0BCFF"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hadith.musaib"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#D0BCFF"
      },
      "userInterfaceStyle": "automatic",
      "versionCode": 6,
      "package": "com.hadith.musaib",
      "googleServicesFile": "./google-services.json"
    },
    "plugins": ["@react-native-firebase/app"],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "b8cbbd1d-8b46-4373-a69e-3a73a98e00a6"
      }
    }
  }
}
